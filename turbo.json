{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "test:unit": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "test:unit:watch": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "test:e2e:headless": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "accessibility": {
      "cache": false
    },
    "accessibility:open": {
      "cache": false
    }
  }
}
