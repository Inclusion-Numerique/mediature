{
  "name": "main",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inclusion-numerique/mediature.git",
    "directory": "apps/main"
  },
  "license": "AGPL-3.0",
  "scripts": {
    "predev": "only-include-used-icons",
    "dev": "next dev",
    "prebuild": "only-include-used-icons",
    "build": "pnpm run db:schema:compile && next build",
    "start": "next start -p $PORT",
    "lint": "TIMING=1 eslint --ext .js,.jsx,.ts,.tsx,.mdx .",
    "clean": "rm -rf .next && rm -rf .turbo && rm -rf node_modules",
    "accessibility": "next export && lhci autorun",
    "accessibility:open": "lhci open",
    "test:prepare": "pnpm run db:schema:compile",
    "test:unit": "jest --ci --passWithNoTests",
    "test:unit:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cross-env PORT=3000 start-server-and-test start http://localhost:3000 cy:open",
    "test:e2e:headless": "cross-env PORT=3000 start-server-and-test start http://localhost:3000 cy:run",
    "cy:run": "cypress run",
    "cy:install": "cypress install",
    "cy:open": "cypress open",
    "db:schema:check": "dotenv-run-script .env.test -- db:schema:check:unsecure",
    "db:schema:check:unsecure": "prisma validate",
    "db:schema:compile": "prisma generate",
    "db:push": "dotenv-run-script .env.test -- db:push:unsecure",
    "db:push:unsecure": "prisma db push",
    "db:seed": "dotenv-run-script .env.test -- db:seed:unsecure",
    "db:seed:unsecure": "prisma db seed",
    "db:migration:prepare": "dotenv-run-script .env.test -- db:migration:prepare:unsecure",
    "db:migration:prepare:unsecure": "prisma migrate dev --create-only",
    "db:migration:deploy": "prisma migrate deploy",
    "db:studio": "dotenv-run-script .env.test -- db:studio:unsecure",
    "db:studio:unsecure": "prisma studio"
  },
  "prisma": {
    "seed": "tsx --tsconfig ../../packages/tsconfig/tsx-fallback.json prisma/seed.script.ts"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^0.0.76",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@mediature/ui": "workspace:ui@*",
    "@mui/material": "^5.10.16",
    "@prisma/client": "^4.7.1",
    "@sentry/integrations": "^7.21.1",
    "@sentry/nextjs": "^7.20.0",
    "@sentry/rrweb": "^0.3.3",
    "@trpc/client": "^10.4.2",
    "@trpc/next": "^10.4.2",
    "@trpc/react-query": "^10.4.2",
    "@trpc/server": "^10.4.2",
    "next": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rrweb": "^1.1.3",
    "tss-react": "^4.4.5"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.15.5",
    "@lhci/cli": "^0.10.0",
    "@octokit/rest": "^19.0.5",
    "@sentry/cli": "^2.9.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@types/testing-library__jest-dom": "^5.14.5",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "cypress": "^11.2.0",
    "dotenv": "^16.0.3",
    "dotenv-run-script": "^0.2.0",
    "eslint-config-custom": "workspace:*",
    "eslint-plugin-testing-library": "^5.9.1",
    "get-tsconfig": "^4.2.0",
    "git-rev-sync": "^3.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-resolver-tsconfig-paths": "^0.14.4",
    "next-transpile-modules": "^10.0.0",
    "parse-github-url": "^1.0.2",
    "prisma": "^4.7.1",
    "start-server-and-test": "^1.14.0",
    "testcontainers": "^9.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "tsx": "^3.12.1",
    "typescript": "^4.9.3"
  }
}
