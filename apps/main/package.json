{
  "name": "main",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "clean": "rm -rf .next && rm -rf .turbo && rm -rf node_modules",
    "accessibility": "next export && lhci autorun",
    "accessibility:open": "lhci open",
    "test:unit": "jest --ci",
    "test:unit:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cross-env PORT=3000 start-server-and-test start http://localhost:3000 cy:open",
    "test:e2e:headless": "cross-env PORT=3000 start-server-and-test start http://localhost:3000 cy:run",
    "cy:run": "cypress run",
    "cy:install": "cypress install",
    "cy:open": "cypress open"
  },
  "dependencies": {
    "@sentry/integrations": "^7.21.1",
    "@sentry/nextjs": "^7.20.0",
    "@sentry/rrweb": "^0.3.3",
    "next": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rrweb": "^1.1.3",
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@lhci/cli": "^0.10.0",
    "@sentry/cli": "^2.9.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^17.0.12",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "cross-env": "^7.0.3",
    "cypress": "^11.2.0",
    "eslint-config-custom": "workspace:*",
    "git-rev-sync": "^3.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "typescript": "^4.5.3"
  }
}
